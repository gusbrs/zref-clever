%   \begin{syntax}
%     \cs{@@_opt_varname_lang_type:nnnn} \Arg{lang} \Arg{ref type}
%     ~~\Arg{option} \Arg{data type}
%   \end{syntax}
%    \begin{macrocode}
\cs_new:Npn \@@_opt_varname_lang_type:nnnn #1#2#3#4
  {
    \@@_language_if_declared:nTF {#1}
      {
        g_@@_opt_lang_
        \tl_use:c { \@@_language_varname:n {#1} }
