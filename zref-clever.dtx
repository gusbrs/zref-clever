\seq_new:N \g_@@_rf_opts_tl_typesetup_seq
\seq_gconcat:NNN \g_@@_rf_opts_tl_typesetup_seq
  \g_@@_rf_opts_tl_maybe_type_specific_seq
  \g_@@_rf_opts_tl_type_names_seq
