    listsep ,
    lastsep ,
    rangesep ,
    namefont ,
    reffont ,
  }
\seq_new:N \g_@@_rf_opts_seq_refbounds_seq
\seq_gset_from_clist:Nn
  \g_@@_rf_opts_seq_refbounds_seq
  {
    refbounds-first ,
